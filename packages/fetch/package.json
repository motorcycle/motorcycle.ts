{
  "name": "@motorcycle/fetch",
  "version": "0.0.0",
  "description": "Isomorphic fetch adapter for Motorcycle.ts",
  "main": "lib/index.js",
  "module": "lib.es2015/index.js",
  "jsnext:main": "lib.es2015/index.js",
  "typings": "lib/index.d.ts",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "cd ../../ && node ./tools/build.js --only fetch",
    "test": "yarn test:lint && yarn test:unit",
    "test:unit": "../../node_modules/.bin/typed-test 'src/*.test.ts' 'src/**/*.test.ts'",
    "test:lint": "../../node_modules/.bin/prettier --write --print-width 100 --tab-width 2 --no-semi --single-quote --trailing-comma es5 --parser typescript src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/motorcyclets/motorcycle.git"
  },
  "keywords": [
    "fetch",
    "isomorphic",
    "motorcycle",
    "node",
    "browser"
  ],
  "author": "Tylor Steinberger <tlsteinberger167@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/motorcyclets/motorcycle/issues"
  },
  "homepage": "https://github.com/motorcyclets/motorcycle#readme",
  "dependencies": {
    "167": "0.37.0",
    "@motorcycle/stream": "1.4.0",
    "@motorcycle/types": "1.3.0",
    "node-fetch": "1.7.2"
  },
  "devDependencies": {
    "@motorcycle/test": "^1.3.0",
    "@types/fetch-mock": "5.12.0",
    "@types/node": "8.0.22",
    "@types/node-fetch": "1.6.7",
    "fetch-mock": "5.12.2"
  }
}
